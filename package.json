{
  "name": "vuepress-starter",
  "version": "0.1.0",
  "description": "vuepress starter",
  "author": "Leon <silenceace@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funnyzak/vuepress-starter.git"
  },
  "keywords": [
    "vuepress",
    "vuepress-starter",
    "document"
  ],
  "scripts": {
    "docs:dev:default": "cross-env CONFIG_NAME=default yarn docs:dev",
    "docs:build:default": "cross-env CONFIG_NAME=default yarn docs:build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "npm:check": "npx npm-check-updates",
    "clean": "npx rimraf node_modules",
    "pangu": "npx pangu -f docs/*",
    "clean:cache": "npx rimraf node_modules/.cache",
    "style:check": "prettier --check --loglevel warn \"./**/*.{js,ts,json,tsx,css,less,vue,html,md,cjs}\"",
    "style:fix": "prettier --write --loglevel warn \"./**/*.{js,ts,json,tsx,css,less,vue,html,md,cjs}\"",
    "eslint:check": "eslint --ext .js,.ts,.vue,.tsx,.cjs ./",
    "eslint:fix": "eslint --fix --ext .js,.ts,.vue,.tsx,.cjs ./",
    "lint:check": "yarn eslint:check && yarn style:check",
    "lint:fix": "yarn eslint:fix && yarn style:fix",
    "lint:staged": "lint-staged",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then husky install; fi"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/glob": "8.0.0",
    "@types/lodash": "4.14.189",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-define-config": "1.12.0",
    "eslint-plugin-vue": "9.7.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "npm-check-updates": "16.4.1",
    "pangu": "4.0.7",
    "prettier": "2.7.1",
    "prettier-config-vuepress": "4.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.9.3",
    "vuepress-plugin-typescript": "0.3.1"
  },
  "dependencies": {
    "@algolia/client-search": "4.14.2",
    "@vuepress/client": "2.0.0-beta.53",
    "@vuepress/core": "2.0.0-beta.53",
    "@vuepress/plugin-docsearch": "2.0.0-beta.53",
    "@vuepress/plugin-google-analytics": "2.0.0-beta.53",
    "@vuepress/plugin-prismjs": "2.0.0-beta.53",
    "@vuepress/plugin-pwa": "2.0.0-beta.53",
    "@vuepress/plugin-register-components": "2.0.0-beta.53",
    "@vuepress/plugin-search": "2.0.0-beta.53",
    "@vuepress/plugin-shiki": "2.0.0-beta.53",
    "@vuepress/utils": "2.0.0-alpha.1",
    "cross-env": "7.0.3",
    "glob": "8.0.3",
    "lodash": "4.17.21",
    "sass-loader": "13.2.0",
    "vue": "3.2.45",
    "vuepress": "2.0.0-beta.53"
  },
  "lint-staged": {
    "*.{js,cjs,,ts}": [
      "prettier --write",
      "eslint --fix --cache"
    ],
    "*.{md,vue,jsx,tsx,html,json,css,less,scss,sass}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "bugs": {
    "url": "https://github.com/funnyzak/vuepress-starter/issues"
  },
  "homepage": "https://github.com/funnyzak/vuepress-starter#readme"
}
